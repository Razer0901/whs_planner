<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>


<?import com.jfoenix.controls.JFXBadge?>
<?import javafx.scene.layout.HBox?>
<?import javafx.geometry.Insets?>
<StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="150.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane fx:id="anchorPane" prefHeight="100.0" prefWidth="150.0">
         <children>
            <JFXButton styleClass="box-button" fx:id="button" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
         </children>
      </AnchorPane>
      <BorderPane mouseTransparent="true">
         <top>
            <Label styleClass="date-label" fx:id="date" text="Date" BorderPane.alignment="TOP_LEFT"/>
         </top>
         <center>
            <HBox fx:id="iconContainer" alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <JFXBadge fx:id="badge" position="TOP_RIGHT" styleClass="icon-badge">
                     <Label fx:id="icon" alignment="CENTER" contentDisplay="CENTER" styleClass="icon">
                        <padding>
                           <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                        </padding>
                     </Label>
                  </JFXBadge>
               </children>
            </HBox>
         </center>
      </BorderPane>
   </children>
</StackPane>
